<!DOCTYPE html>
<html>
<head>
  <title>Nimbl</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body ng-app="app">
  <%= render 'partials/header' %>
  <div class="container-fluid" ng-controller="<%= params[:controller] %>Controller">
    <%= yield %>
  </div>
</body>
</html>
